<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title></title>
  </head>
  <body>

    {{#each linksdata}}
      <a onclick="update('{{this.id}}')" href="{{this.link.url}}">
        {{this.link.title}}
      </a>
    {{/each}}

  </body>

  <script>

    function update(id) { var xhr = new XMLHttpRequest(); xhr.open('POST',
    '/links/trackLink/' + id); xhr.setRequestHeader('Content-Type',
    'application/json'); xhr.send(JSON.stringify({ id: id })); }

  </script>

</html>